{
  "name": "Social Browsing",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Browse web pages with friends.",
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/",
    "<all_urls>"
  ],
  "browser_action": {
      "default_title": "Social Browsing",
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": [
        "jquery.js",
        "background.js"
      ],
      "persistent": true
  }
}
